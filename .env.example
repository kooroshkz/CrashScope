# CrashScope Environment Configuration Template
# Copy this file to .env and fill in your actual values

# TomTom API Configuration (Required)
TOMTOM_API_KEY=your_tomtom_api_key_here

# Cache and Timeout Settings (Optional)
CACHE_TIMEOUT=600          # Cache timeout in seconds (default: 10 minutes)
REQUEST_TIMEOUT=10         # API request timeout in seconds (default: 10 seconds)

# Development Settings (Optional)
DEBUG=false               # Enable debug logging
LOG_LEVEL=INFO           # Logging level: DEBUG, INFO, WARNING, ERROR